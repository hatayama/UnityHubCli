{
  "name": "unity-hub-cli",
  "version": "0.22.0",
  "description": "A CLI tool that reads Unity Hub's projects and launches Unity Editor with an interactive TUI",
  "main": "dist/index.js",
  "scripts": {
    "test": "vitest run",
    "dev": "tsx src/index.ts",
    "build": "tsup",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "typecheck": "tsc -noEmit",
    "allow-scripts": "allow-scripts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hatayama/UnityHubCli.git"
  },
  "keywords": [
    "unity",
    "unity-hub",
    "cli",
    "tui",
    "ink",
    "terminal",
    "interactive"
  ],
  "author": "masamichi hatayama",
  "license": "MIT",
  "type": "module",
  "bin": {
    "unity-hub-cli": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "chalk": "5.6.2",
    "clipboardy": "5.3.0",
    "ink": "6.7.0",
    "launch-unity": "0.15.1",
    "react": "19.2.4"
  },
  "engines": {
    "node": ">=18"
  },
  "lavamoat": {
    "allowScripts": {
      "eslint-import-resolver-typescript>unrs-resolver": false,
      "tsup>bundle-require>esbuild": false,
      "tsup>esbuild": false,
      "tsx>esbuild": false
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@lavamoat/allow-scripts": "3.4.2",
    "@types/node": "25.2.0",
    "@types/react": "19.2.10",
    "@typescript-eslint/eslint-plugin": "8.54.0",
    "@typescript-eslint/parser": "8.54.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "prettier": "3.8.1",
    "tsup": "8.5.1",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.48.1",
    "vitest": "4.0.18"
  }
}
