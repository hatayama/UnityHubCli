{
  "name": "unity-hub-cli",
  "version": "0.1.0",
  "description": "A CLI tool that reads Unity Hub's projects and launches Unity Editor with an interactive TUI",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "tsx src/index.ts",
    "build": "tsup",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "typecheck": "tsc -noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hatayama/UnityHubCli.git"
  },
  "keywords": [
    "unity",
    "unity-hub",
    "cli",
    "tui",
    "ink",
    "terminal",
    "interactive"
  ],
  "author": "masamichi hatayama",
  "license": "MIT",
  "type": "module",
  "bin": {
    "unity-hub-cli": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "clipboardy": "^4.0.0",
    "ink": "^4.4.1",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.19.20",
    "@types/react": "^18.3.26",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.32.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
